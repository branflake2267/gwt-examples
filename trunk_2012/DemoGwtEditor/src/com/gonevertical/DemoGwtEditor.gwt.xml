<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='demogwteditor'>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>


  <!-- Inherit the default GWT style sheet.  You can change       -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>


  <!-- Other module inherits                                      -->
  <inherits name='com.google.web.bindery.requestfactory.RequestFactory' />
  <inherits name="com.google.gwt.activity.Activity"/>
  <inherits name="com.google.gwt.place.Place"/>
  <inherits name='com.google.gwt.editor.Editor'/>


  <!-- my core module 
  <inherits name='org.gonevertical.core.Core'/> -->


  <!-- load css com/gonevertical/public/css/style.css -->
  <stylesheet src="css/style.css"/>


  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.gonevertical.client.InitApp'/>


  <!-- Use ApplicationFactoryImpl by default (Deferred Binding) -->
  <replace-with class="com.gonevertical.client.app.ClientFactoryImpl">
    <when-type-is class="com.gonevertical.client.app.ClientFactory"/>
  </replace-with>


  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <!-- <source path='shared'/> -->


</module>
