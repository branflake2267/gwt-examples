<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:org.gonevertical.core.client.input">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel>
		<g:FocusPanel ui:field="pFocus">
			<g:HorizontalPanel width="100%" height="100%">
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<p1:WiseTextBox width="365px" ui:field="tbName"/>
				</g:Cell>
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:HTML wordWrap="true">&nbsp;</g:HTML>
				</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER">
				<g:PushButton text="X" ui:field="bDelete"/>
			</g:Cell>
		</g:HorizontalPanel>
		</g:FocusPanel>
	</g:VerticalPanel>
</ui:UiBinder> 