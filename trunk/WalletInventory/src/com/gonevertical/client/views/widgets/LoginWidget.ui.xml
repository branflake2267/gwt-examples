<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HorizontalPanel>
		<g:Cell verticalAlignment="ALIGN_MIDDLE">
			<g:HTML wordWrap="true" ui:field="htmlNick">&nbsp;</g:HTML>
		</g:Cell>
		<g:HTML wordWrap="true">&nbsp;&nbsp;</g:HTML>
		<g:Cell verticalAlignment="ALIGN_MIDDLE">
			<g:HTML wordWrap="true" ui:field="htmlUrl">&nbsp;</g:HTML>
		</g:Cell>
		<g:HTML wordWrap="true">&nbsp;</g:HTML>
		<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT">
			<g:Image ui:field="imgHelp" url="images/help.png"/>
		</g:Cell>
	</g:HorizontalPanel>
</ui:UiBinder> 