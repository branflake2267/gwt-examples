<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:PushButton text="I&apos;m Finished" ui:field="bFinished"/>
		</g:HorizontalPanel>
		<g:FocusPanel ui:field="pFocusName">
			<g:HorizontalPanel width="100%" height="100%">
			<g:Cell verticalAlignment="ALIGN_MIDDLE">
				<g:HTML wordWrap="true">My Wallet:&nbsp;</g:HTML>
			</g:Cell>
			<g:VerticalPanel width="100%">
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:HTML wordWrap="true" ui:field="htmlName" width="100%">Name</g:HTML>
				</g:Cell>
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:TextBox width="302px" ui:field="tbName"/>
				</g:Cell>
			</g:VerticalPanel>
		</g:HorizontalPanel>
		</g:FocusPanel>
		<g:HorizontalPanel>
			<g:PushButton text="Add" ui:field="bAdd"/>
			<g:Cell verticalAlignment="ALIGN_MIDDLE">
				<g:HTML wordWrap="true">&nbsp;My Wallet Items</g:HTML>
			</g:Cell>
		</g:HorizontalPanel>
		<g:VerticalPanel ui:field="pList"/>
	</g:VerticalPanel>
</ui:UiBinder> 